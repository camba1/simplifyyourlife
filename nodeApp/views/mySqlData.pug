extends default

block content
  div(class="columns")
    div(class="column is-5 is-offset-3")
      h4(class="title is-4 has-text-centered") Database countries list
      table(class="table is-bordered is-fullwidth is-striped")
        tbody
          tr
            th Id
            th Name
          each country in countryList
            tr
              td #{country.id}
              td #{country.name}
